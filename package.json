    "vite": "^7.2.6",
    "vite-plugin-svg-icons": "^2.0.1",
    "vue": "^3.5.25",
    "vue-eslint-parser": "^10.2.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "electron",
      "esbuild"
    ]
